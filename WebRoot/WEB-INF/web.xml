<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name></display-name>
  <filter>
    <filter-name>encodingFilter</filter-name>
    <filter-class>com.zqc.util.EncodingFilter</filter-class>
    <init-param>
      <param-name>Encoding</param-name>
      <param-value>utf-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>encodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>AlipayServlet</servlet-name>
    <servlet-class>com.zqc.servlet.AlipayServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AlipayServlet</servlet-name>
    <url-pattern>/AlipayServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>NotifyServlet</servlet-name>
    <servlet-class>com.zqc.servlet.NotifyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NotifyServlet</servlet-name>
    <url-pattern>/NotifyServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>ValidateUser</servlet-name>
    <servlet-class>com.zqc.servlet.ValidateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ValidateUser</servlet-name>
    <url-pattern>/ValidateServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GetExcelServlet</servlet-name>
    <servlet-class>com.zqc.servlet.GetExcelServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetExcelServlet</servlet-name>
    <url-pattern>/GetExcelServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AdminOperationServlet</servlet-name>
    <servlet-class>com.zqc.servlet.AdminOperationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminOperationServlet</servlet-name>
    <url-pattern>/AdminOperationServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ReturnServlet</servlet-name>
    <servlet-class>com.zqc.servlet.ReturnServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FreshmanQueryServlet</servlet-name>
    <servlet-class>com.zqc.servlet.FreshmanQueryServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ReturnServlet</servlet-name>
    <url-pattern>/ReturnServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FreshmanQueryServlet</servlet-name>
    <url-pattern>/FreshmanQueryServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>